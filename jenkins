pipeline{
agent any
stages{
stage ('build'){
steps{
sleep 5
echo " build stage"
}
}
stage ('test'){
steps{
script {
                    catchError(buildResult: 'FAILURE', stageResult: 'FAILURE') {
                        sh 'sleep 3; echo "This is test stage"'
                    }
}
  }
 }
stage ('deploy'){
steps{
sleep 2
echo "deploy stage"

}
}

}
}

